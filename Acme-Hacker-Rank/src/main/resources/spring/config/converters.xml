<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<bean class="converters.AdministratorToStringConverter" />
		<bean class="converters.StringToAdministratorConverter" />

		<bean class="converters.AuthorityToStringConverter" />
		<bean class="converters.StringToAuthorityConverter" />

		<bean class="converters.UserAccountToStringConverter" />
		<bean class="converters.StringToUserAccountConverter" />

		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />
		
		<bean class="converters.HackerToStringConverter" />
		<bean class="converters.StringToHackerConverter" />
		
		<bean class="converters.ApplicationToStringConverter" />
		<bean class="converters.StringToApplicationConverter" />
		
		<bean class="converters.PositionToStringConverter" />
		<bean class="converters.StringToPositionConverter" />
		
		<bean class="converters.SocialIdentityToStringConverter" />
		<bean class="converters.StringToSocialIdentityConverter" />
		
		<bean class="converters.ProblemToStringConverter" />
		<bean class="converters.StringToProblemConverter" />

		<bean class="converters.ConfigurationsToStringConverter" />
		<bean class="converters.StringToConfigurationsConverter" />
		
		<bean class="converters.MessageToStringConverter" />
		<bean class="converters.StringToMessageConverter" />

		<bean class="converters.UrlToStringConverter" />
		<bean class="converters.StringToUrlConverter" />
		
		<bean class="converters.CompanyToStringConverter" />
		<bean class="converters.StringToCompanyConverter" />

	</util:list>

</beans>